{% if page.fileSlug == "Galeria de Documentos" %}

<h2>Image Files from Custom Collection</h2>

<ul>
  {% for file in collections.orneuImages %}
    <li>
      <strong>{{ file.name }}</strong> - <a href="{{ file.path }}" target="_blank">{{ file.path }}</a>
    </li>
  {% endfor %}
</ul>

{% endif %}
