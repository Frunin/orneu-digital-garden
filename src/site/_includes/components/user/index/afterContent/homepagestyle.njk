{% import "components/filetree.njk" as FileTree %}

<h2>Debugging filetree on homepage</h2>

<pre>{{ filetree | dump }}</pre>

{% if filetree %}
  <div>
    {% for key, node in filetree | dictsort %}
      {{ FileTree.menuItem(key, node, 0, key) }}
    {% endfor %}
  </div>
{% else %}
  <p>No filetree data found.</p>
{% endif %}
