<h2>Filetree Raw Data</h2>
<pre>{{ filetree | safe | dump(4) }}</pre>

<h2>Filetree Simple List</h2>
<ul>
  {% if filetree and filetree.Orneu %}
    {% for key, node in filetree.Orneu %}
      <li>
        {{ key }} - 
        {% if node.isNote %}
          Note: <a href="{{ node.permalink }}">{{ node.name }}</a>
        {% elif node.isFolder %}
          Folder
          <ul>
            {% for childName, childNode in node %}
              {% if childName not in ['isNote', 'isFolder'] %}
                <li>{{ childName }}</li>
              {% endif %}
            {% endfor %}
          </ul>
        {% else %}
          Unknown type
        {% endif %}
      </li>
    {% endfor %}
  {% else %}
    <li>No filetree.Orneu data found</li>
  {% endif %}
</ul>
